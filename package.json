{
  "name": "vue-bleeding-edge-starter",
  "version": "21.2.1",
  "description": "Starter for Vue.js projects with understandable webpack config and bleeding edge updated dependencies",
  "main": "client/index.js",
  "author": "Lefteris Pegiadis",
  "license": "MIT",
  "scripts": {
    "hot": "cross-env NODE_ENV=development webpack serve --hot --inline --progress",
    "dev": "cross-env NODE_ENV=development webpack serve --progress",
    "build": "cross-env rm -rf public && cross-env NODE_ENV=production webpack --progress",
    "lint": "eslint client/**/*.{js,vue}",
    "prettier-check": "prettier --list-different client/**/*.{js,vue,css,scss}",
    "prettify": "prettier --write client/**/*.{js,vue,css,scss}",
    "test": "exit 0"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/plugin-transform-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^1.3.8",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ]
}
